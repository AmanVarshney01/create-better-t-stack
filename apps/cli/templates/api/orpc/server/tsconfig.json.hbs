{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": [
      {{#if (eq runtime "node")}}
        "node"
      {{else if (eq runtime "bun")}}
        "bun"
      {{else if (eq runtime "workers")}}
        "node"
      {{else}}
        "node",
        "bun"
      {{/if}}{{#if (eq serverDeploy "alchemy")}},
        "@cloudflare/workers-types"
      {{/if}}
    ]
  }
}