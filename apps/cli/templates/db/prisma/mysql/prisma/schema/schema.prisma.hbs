generator client {
  provider = "prisma-client"
  output   = "../generated"
  moduleFormat = "esm"
  {{#if (eq runtime "bun")}}
  runtime = "bun"
  {{/if}}
  {{#if (eq runtime "node")}}
  runtime = "nodejs"
  {{/if}}
  {{#if (eq runtime "workers")}}
  runtime = "workerd"
  {{/if}}
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
