{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"declaration": true,
		"declarationMap": true,
		"noEmit": true,
		"paths": {
			"~encore/*": ["./encore.gen/*"]
		}
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules"]
}
