{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"types": ["nitropack"]
	},
	"include": ["**/*.ts", "nitro.config.ts"],
	"exclude": ["node_modules", ".output", ".nitro"]
}
